{
  "name": "@warp-works/studio",
  "version": "0.7.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/WarpWorks/studio.git"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "author": {
    "name": "Dirk Slama",
    "email": "dirk.slama@gmx.de"
  },
  "contributors": [
    {
      "name": "Hữu Ðà Trần",
      "email": "huuda.tran@quoininc.com",
      "url": "https://www.quoininc.com/staff/huu-da-tran/"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "watch": "npm-watch",
    "lint:node": "eslint --fix --ignore-pattern *.test.js --ignore-pattern server/test lib server",
    "lint:test": "eslint --fix --config=.eslintrc.test.json  '**/*.test.js'",
    "lint:client": "eslint --fix --config=.eslintrc.client.json public/WarpWorks",
    "lint": "npm run -s lint:node && npm run -s lint:test",
    "test:unit": "mocha 'lib/**/*.unit.test.js'",
    "test": "npm run test:unit",
    "coverage:unit": "nyc --report-dir=coverage/unit npm run test:unit",
    "coverage": "npm run coverage:unit"
  },
  "watch": {
    "coverage:unit": {
      "patterns": [
        "lib",
        "server"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "dependencies": {
    "@quoin/expressjs-routes-info": "0.1.5",
    "@warp-works/core": "0.7.2",
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "bootstrap-notify": "3.1.3",
    "cookie-parser": "1.4.3",
    "debug": "2.6.3",
    "hal": "0.1.0",
    "hbs": "4.0.1",
    "hbs-utils": "0.0.4",
    "lodash": "4.17.4",
    "mongodb": "2.2.25",
    "rc": "1.2.1",
    "serve-favicon": "2.4.2"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "0.1.1",
    "@quoin/node-test-helpers": "0.1.7",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "express": "4.15.2",
    "mocha": "3.2.0",
    "npm-watch": "0.1.8",
    "nyc": "10.2.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  }
}
